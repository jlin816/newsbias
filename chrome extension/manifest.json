{
  "manifest_version": 2,

  "name": "News Bias",
  "description": "This extension will analyze a page using GTmetrix",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"],
   "content_security_policy":"script-src 'self' https://access.alchemyapi.com; object-src 'self' "
  
}